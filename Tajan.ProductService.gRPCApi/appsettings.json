{
  "ConnectionStrings": {
    "Tajan_Product_Db": "Server=localhost;Database=Tajan_ProductDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "UseInMemoryDataBase": true,
  "Redis": {
    "ServiceName": "",
    "MasterNode": {
      "Host": "localhost",
      "Port": "6379"
    },
    "ReplicaNodes": [
      {
        "Host": "localhost",
        "Port": "6379"
      }
    ],
    "Resiliency": {
      "TimeoutInMilliseconds": 300,
      "RetryCount": 3,
      "SamplingDurationToCheckForFailureInSeconds": 20,
      "FailureRatioToBreakTheCircuit": 0.8,
      "MinimumThroughputToCheckForFailure": 5,
      "BreakDurationInSeconds": 60
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
