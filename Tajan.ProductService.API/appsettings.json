{
  "ConnectionStrings": {
    "Tajan_Product_Db": "Server=localhost;Database=Tajan_ProductDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "UseInMemoryDataBase": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MySettings": {
    "StringSetting": "My Value 9",
    "IntSetting": 23
  },
  "Redis": {
    "ServiceName": "",
    "MasterNode": {
      "Host": "localhost",
      "Port": "6379"
    },
    "ReplicaNodes": [
      {
        "Host": "localhost",
        "Port": "6379"
      }
    ],
    "Resiliency": {
      "TimeoutInMilliseconds": 300,
      "RetryCount": 3,
      "SamplingDurationToCheckForFailureInSeconds": 20,
      "FailureRatioToBreakTheCircuit": 0.8,
      "MinimumThroughputToCheckForFailure": 5,
      "BreakDurationInSeconds": 60
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.MSSqlServer"
    ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "MSSqlServer",
        "Args": {
          "autoCreateSqlTable": true,
          "ConnectionString": "LoggerDb",
          "tableName": "Logs"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "ApplicationName": "Your ASP.NET Core App"
    }
  },
  "AllowedHosts": "*"
}
